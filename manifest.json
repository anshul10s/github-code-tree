{
    "name": "Github Code Tree",
    "version": "0.1.0",
    "description": "Show the source tree on the github PR",
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
          "lib/jquery-1.11.1.min.js",
          "app/icons.js",
          "app/style.js",
          "app/app.js"
        ]
      }
    ],

    "permissions": [
      "tabs",
      "http://*github*/*",
      "https://*github*/*"
    ],
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png"
    }
  }
